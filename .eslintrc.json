{
  "extends": ["airbnb"],
  "overrides": [
    {
      "files": ["src/**/*.js"],
      "rules": {
        "react/forbid-prop-types": 0,
        "react/jsx-props-no-spreading": "off",
        "default-param-last": 0,
        "no-param-reassign": 0,
        "no-shadow": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/extensions": [
          "error",
          "ignorePackages"
        ]
      }
    }
  ]
}
